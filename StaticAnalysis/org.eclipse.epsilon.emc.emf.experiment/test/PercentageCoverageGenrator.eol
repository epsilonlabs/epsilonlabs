}

eolQueryString = eolQueryString + "size.println();\n";
eolQueryString.println();


operation getPercent(seq: Sequence, size: Integer, percent : Integer)
{
	var result = new Set;
	var number = size*percent/100;
	var sum = 0;
	for(k in seq)
	{
		var value = k.at(1);
		if(value < number)
		{
			if(value + sum <= number or value + sum < number * 1.05)
			{
				sum = sum + value;
				result.add(k.at(0));
			}
		}
		if(sum > number)
		{
			break;
		}
	}
	sum.errln();
	return result;
}